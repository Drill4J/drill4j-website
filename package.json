    "babel-plugin-macros": "^3.0.1",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.1.11",
    "postcss": "^8.2.7",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "tailwindcss": "^2.0.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
