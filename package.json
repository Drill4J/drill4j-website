    "clear": "docusaurus clear",
    "tailwind:css": "tailwind build src/css/tailwind.css -c tailwind.config.js -o src/css/index.css"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
