{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run tailwind:css && docusaurus start",
    "build": "NODE_ENV=production npm run tailwind:css && docusaurus build",
    "swizzle": "docusaurus swizzle",
